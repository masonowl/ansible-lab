---
ansible-navigator:
  execution-environment:
    container-engine: podman
    image: ee-supported-rhel8
    enabled: false
    # pull-policy: never

  playbook-artifact:
    save-as: /home/rhel/playbook-artifacts/{playbook_name}-artifact-{ts_utc}.json

  logging:
    level: debug

  editor:
    command: code-server {filename}
    console: false